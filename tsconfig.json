{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "sourceMap": false,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "moduleResolution": "node"
  },
  "lib": [
    "dom",
    "es6",
    "es2015.promise"
  ]
}